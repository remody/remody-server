type User {
	id: ID! @unique
	name: String!
	email: String! @unique
	password: String!
	Files: [File!]! @relation(name: "FileToUser", onDelete: CASCADE)
	interpretedDatas: [InterpretedData!]!
		@relation(name: "InterpretedDataToUser", onDelete: CASCADE)
}

type File {
	id: ID! @unique
	filename: String!
	mimetype: String!
	encoding: String!
	path: String! @unique
	owner: User! @relation(name: "FileToUser", onDelete: SET_NULL)
}

type InterpretedData {
	id: ID! @unique
	name: String!
	url: String!
	description: String!
	owner: User! @relation(name: "InterpretedDataToUser", onDelete: SET_NULL)
}
